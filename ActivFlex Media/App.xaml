<Application x:Class="ActivFlex.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Images/Logos.xaml"/>
                <ResourceDictionary Source="Images/TitleBarIcons.xaml"/>
                <ResourceDictionary Source="Images/NavIcons.xaml"/>
                <ResourceDictionary Source="Images/BrowseIcons.xaml"/>
                <ResourceDictionary Source="Styles/Fonts.xaml"/>
                <ResourceDictionary Source="Styles/Thumbnails.xaml"/>
                <ResourceDictionary Source="Styles/ContextMenus.xaml"/>
                <ResourceDictionary Source="Styles/BrowseNavigation.xaml"/>
                <ResourceDictionary Source="Styles/TreeViewItem.xaml"/>
                <ResourceDictionary Source="Styles/ToggleButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
